<?xml version='1.0' encoding='utf-8'?>
<widget id="io.catalpa.hamutukSetup" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>hamutukSetup</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>

    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <preference name="orientation" value="portrait" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <hook src="scripts/grunt.sh" type="before_build" />
    <hook src="scripts/grunt.sh" type="before_run" />
    <engine name="android" spec="~6.1.2" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-battery-status" spec="~1.2.4" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.3" />
    <plugin name="phonegap-plugin-barcodescanner" spec="~6.0.6" />
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.6.3" />
    <plugin name="cordova-plugin-streaming-media" spec="1.0.2" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-fabric-plugin" spec="~1.1.7">
        <variable name="FABRIC_API_KEY" value="abcderf1234" />
        <variable name="FABRIC_API_SECRET" value="abcderf1234" />
    </plugin>
    <plugin name="cordova-plugin-code-push" spec="~1.9.4-beta" />
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-plugin-buildinfo" spec="~1.1.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
    <plugin name="cordova-plugin-datepicker" spec="~0.9.3" />
    <plugin name="cordova.plugins.diagnostic" spec="~3.7.2" />
    <plugin name="cordova-plugin-request-location-accuracy" spec="~2.2.2" />
</widget>
